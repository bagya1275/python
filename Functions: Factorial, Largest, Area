def factorial(n):
    result = 1
    for i in range(1, n + 1):
        result *= i
    return result

def largest(lst):
    return max(lst)

def area_rectangle(length, width):
    return length * width

print("Factorial:", factorial(5))
print("Largest:", largest([10, 30, 20]))
print("Area of rectangle:", area_rectangle(5, 4))
