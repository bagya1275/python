from utilities import math_utils, string_utils

print("Addition:", math_utils.add(10, 5))
print("Subtraction:", math_utils.subtract(10, 5))
print("Upper:", string_utils.to_upper("hello"))
print("Lower:", string_utils.to_lower("HELLO"))
